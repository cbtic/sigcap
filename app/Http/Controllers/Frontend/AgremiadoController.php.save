	public function equivalenciaIdioma($idioma){
		
		$idioma_nuevo = 0;
		
		switch ($idioma) {
		  case "19":
			$idioma_nuevo=1;//INGLES
			break;
		  case "20":
			$idioma_nuevo=2;//FRANCES
			break;
		  case "21":
			$idioma_nuevo=3;//ITALIANO
			break;
		  case "22":
			$idioma_nuevo=4;//PORTUGUES
			break;
		  case "23":
			$idioma_nuevo=5;//ALEMï¿½N
			break;
		  case "24":
			$idioma_nuevo=999;//OTROS
